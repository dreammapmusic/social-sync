# Frontend OAuth Configuration
# Copy this file to .env and fill in your OAuth credentials

# Facebook OAuth (also used for Instagram)
# Get from: https://developers.facebook.com/apps/
VITE_FACEBOOK_CLIENT_ID=your_facebook_app_id_here

# Twitter OAuth  
# Get from: https://developer.twitter.com/en/portal/dashboard
VITE_TWITTER_CLIENT_ID=your_twitter_client_id_here

# LinkedIn OAuth
# Get from: https://www.linkedin.com/developers/apps
VITE_LINKEDIN_CLIENT_ID=your_linkedin_client_id_here

# Google OAuth (used for YouTube)
# Get from: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Backend API URL (optional override)
VITE_API_URL=https://social-sync-api.norbert-gobor3.workers.dev 